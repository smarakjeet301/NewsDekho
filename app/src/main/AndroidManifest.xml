            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme" />
        <activity android:name=".DetailsActivity" />
        <activity android:name=".BusinessActivity" />
        <activity android:name=".Healthactivity" />
        <activity android:name=".ScienceActivity" />
        <activity android:name=".TechnologyActivity" />
        <activity android:name=".SportsActivity" />
        <activity android:name=".EntertainmentActivity" />
        <activity android:name=".Dashboard" />
        <activity android:name=".JokesActivity" />
        <activity android:name=".WeatherActivity" />
        <activity android:name=".HoroscopeActivity" />
